universe    = vanilla     
executable  = run_py.sh
arguments = $(Process) $(number)

transfer_input_files = increment.py

log         = job.log
output      = job.out
error       = job.error

# Require nodes that can access the correct OSG modules
Requirements = (HAS_MODULES =?= true) && (OSGVO_OS_STRING == "RHEL 7")

request_cpus    = 1 
request_memory  = 2GB
request_disk    = 2GB

queue number from inputs.txt

